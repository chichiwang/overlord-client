// nib is a powerful lib for stylus that provides cross-browser css3 mixins.
// No more vendor prefixes. http://visionmedia.github.com/nib/
@import 'nib'

@font-face
  font-family: 'icons'
  src:url('/fonts/icomoon.eot?lkw1t7')
  src:url('/fonts/icomoon.eot?#iefixlkw1t7') format('embedded-opentype'),
      url('/fonts/icomoon.ttf?lkw1t7') format('truetype'),
      url('/fonts/icomoon.woff?lkw1t7') format('woff'),
      url('/fonts/icomoon.svg?lkw1t7#icomoon') format('svg');
  font-weight: normal
  font-style: normal

[class^="icon-"], [class*=" icon-"]
  font-family: 'icons'
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale
  
.icon-spin:before
  content: "\e600"
.icon-arrow-up:before
  content: "\ea32"
.icon-arrow-down:before
  content: "\ea36"
.icon-skeletor:before
  content: "\e601"

@import '_globals'
@import '../components/root/_root'
@import '../components/statusbar/_statusbar'
@import '../components/stage/_stage'
@import '../components/menu/_menu'
@import '../components/console/_console'
@import '../components/screen/_screen'